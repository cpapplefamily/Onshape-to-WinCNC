{
  "line_patterns": [
    {
      "match": "^[Oo]\\d+.*$",
      "action": "comment",
      "prefix": "[",
      "suffix": "]"
    },
    {
      "match": "^%.*$",
      "action": "comment",
      "prefix": "[",
      "suffix": "]"
    }
  ],

  "token_replacements": {
    "_remove_toolchange": "M6 is removed because ShopSabre has no ATC and tool changes are handled using a Macro instead T#.",
    "M6": "",
    "M7": "M11C8 [ ;Coolant On ]",
    "M8": "M11C8 [ ;Coolant On ]",
    "M9": "M12C8 [ ;Coolant Off ]",
    "G17": "[ G17 ;XY Plane Selection ]",
    "G18": "[ G18 ;XZ Plane Selection ]",
    "G19": "[ G19 ;YZ Plane Selection ]",
    "G40": "[ G40 ;Tool Radius Compensation Off ]",
    "G80": "G80 [ ;Cancel Canned Cycle ]",
    "G49": "[ G49 ;Tool Length Offset Cancel ]",
    "G98": "[ G98 ;Return to Initial Point in Canned Cycle ]",
    "H1": "[ H1 ;Tool Length Offset 1 ]",
    "H2": "[ H2 ;Tool Length Offset 2 ]",
    "H3": "[ H3 ;Tool Length Offset 3 ]",
    "H4": "[ H4 ;Tool Length Offset 4 ]",
    "H5": "[ H5 ;Tool Length Offset 5 ]",
    "H6": "[ H6 ;Tool Length Offset 6 ]",
    "_comment_T7_to_T16": "Only comment tool numbers used on the 6-station rack",
    "(T(?:[7-9]|1[0-6]))": "[\\1]",
    "(N\\d+)": "[\\1]"
  }
}
